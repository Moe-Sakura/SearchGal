{
    "functions": {
        "api/**/*.ts": {
            "includeFiles": "src/**/*"
        }
    },
    "rewrites": [
        {
            "source": "/gal",
            "destination": "/api/gal"
        },
        {
            "source": "/patch",
            "destination": "/api/patch"
        },
        {
            "source": "/",
            "destination": "/api/index"
        }
    ]
}